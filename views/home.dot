<div class="container">
    <div class="row">
        <div class="col-md-9"><h2>Competities</h2></div>
    </div>
    <div class="row">
        {{~it.competition:l}}
        {{?l}}
        <div class="col-md-4">
            <div class="list-group">
                <a href="#" class="list-group-item competition">{{=l.name}}</a>
                {{~l.divisions:d}}
                {{?d}}
                <a href="/agenda/{{=d.name.cleanup()}}/" class="list-group-item">
                    {{=d.name.undash()}}
                </a>
                {{?}}
                {{~}}
            </div>

        </div>
        {{?}}
        {{~}}
    </div>
</div>